<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_fp_send_pending_documents" model="ir.cron">
        <field name="name">FE CR - Enviar pendientes a Hacienda</field>
        <field name="model_id" ref="account.model_account_move"/>
        <field name="state">code</field>
        <field name="code">model._fp_cron_send_pending_documents()</field>
        <field name="active">True</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
    </record>

    <record id="ir_cron_fp_consult_pending_documents" model="ir.cron">
        <field name="name">FE CR - Consultar estados pendientes</field>
        <field name="model_id" ref="account.model_account_move"/>
        <field name="state">code</field>
        <field name="code">model._fp_cron_consult_pending_documents()</field>
        <field name="active">True</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
    </record>
</odoo>
